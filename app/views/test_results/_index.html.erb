<table class="table table-striped table-bordered">
  <tr>
    <th>Nitrate</th>
    <th>Nitrite</th>
    <th>Hardness</th>
    <th>Alkalinity</th>
    <th>pH</th>
    <th></th>
  </tr>
  <%= render 'test_results/form' %>
  <% @test_results.each do |test_result| %>
    <tr>
      <td><%= test_result.nitrate %></td>
      <td><%= test_result.nitrite %></td>
      <td><%= test_result.hardness %></td>
      <td><%= test_result.alkalinity %></td>
      <td><%= test_result.ph %></td>
      <td>
        <%= link_to [@bowl, test_result], confirm: 'Are you sure?', method: :delete do %>
          <i class="icon-remove"></i> Delete
        <% end %>
      </td>
    </tr>
  <% end %>
</table>
