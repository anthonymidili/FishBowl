<table class="table table-striped table-bordered">
  <tr>
    <th></th>
    <th>Nitrate</th>
    <th>Nitrite</th>
    <th>Hardness</th>
    <th>Alkalinity</th>
    <th>pH</th>
    <th></th>
  </tr>
  <%= render 'test_results/form' %>
  <% @test_results.each do |test_result| %>
    <tr>
      <td><%= test_result.created_at %></td>
      <td><%= test_result.nitrate %></td>
      <td><%= test_result.nitrite %></td>
      <td><%= test_result.hardness %></td>
      <td><%= test_result.alkalinity %></td>
      <td><%= test_result.ph %></td>
      <td>
        <%= link_to "Delete Results", [@bowl, test_result],
                    class: "btn btn-small btn-danger",
                    confirm: 'Are you sure?',
                    method: :delete %>
      </td>
    </tr>
  <% end %>
</table>
